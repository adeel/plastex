name: itemize
<ul>
  {{ obj }}
</ul>

name: enumerate
<ol>
  {{ obj }}
</ol>

name: description
<dl>
{% for item in obj %}
  <dt>{{ item.attributes.term or obj.attributes.defaultlabel }}</dt>
  <dd>{{ item }}</dd>
{% endfor %}
</dl>

name: item
<li {% if obj.attributes['term'] %}class="custom-item" data-marker="{{ obj.attributes['term'] }}"{% endif %}>{{ obj }}</li>
{# TODO handle labels, tags #}
