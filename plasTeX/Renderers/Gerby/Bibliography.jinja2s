name: cite
<span class="cite">
[
{# TODO this needs attention: plasTeX needs to be aware of the BibTeX for it to work? #}
{% for item in obj.citation() %}
  {% if item.idref and item.idref.bibitem.url %}
    <cite><a href="{{ item.idref.bibitem.url }}">{{ item }}</a></cite>
  {% else %}
    {{ item }}
  {% endif %}
{% endfor %}
]
</span>

